{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "dingo/api": "^2.4",
        "douyasi/laravel-wang-editor": "^2.4",
        "fideloper/proxy": "^4.0",
        "finecho/logistics": "^3.0",
        "guzzlehttp/guzzle": "^6.5",
        "intervention/image": "^2.5",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^6.0",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "medz/cors": "^1.4",
        "overtrue/easy-sms": "^1.1",
        "overtrue/laravel-wechat": "^5.0",
        "predis/predis": "^1.1",
        "simplesoftwareio/simple-qrcode": "^2.0",
        "tymon/jwt-auth": "1.*@rc",
        "yansongda/laravel-pay": "^2.1",
        "yansongda/pay": "^2.8",
        "zhuzhichao/bank-card-info": "^2.1"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Services",
            "app/Services/AdminService",
            "app/Services/WebService",
            "app/Services/RedisService"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
